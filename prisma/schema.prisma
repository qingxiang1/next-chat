generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

model User {
    id       String   @id @default(uuid())
    username String
    password String?
    ip       String?
    sys_type String?
    created  DateTime @default(now())
    updated  DateTime @updatedAt
}
